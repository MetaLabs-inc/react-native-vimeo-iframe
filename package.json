{
  "name": "@conquerplus/react-native-web-video",
  "version": "1.0.5",
  "description": "Library to render videos in a React Native app as webview listening all events supported on it",
  "homepage": "https://github.com/conquer-plus/react-native-web-video#readme",
  "main": "lib/commonjs/index.js",
  "types": "lib/typescript/index.d.ts",
  "module": "lib/module/index.js",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "author": "Marco Fiorito <marcofiorito1@gmail.com>, Douglas Rosa <douglasrosa0110@gmail.com>",
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "babel.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/conquer-plus/react-native-web-video"
  },
  "keywords": [
    "react native",
    "vimeo",
    "video",
    "webview"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "compile": "rm -rf lib && tsc -p .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.63.46",
    "@types/react-test-renderer": "^17.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.0",
    "babel-cli": "^6.26.0",
    "react": "^17.0.1",
    "react-native": "^0.63.4",
    "react-test-renderer": "^17.0.1",
    "react-native-webview": "11.2.3",
    "typescript": "^4.1.3",
    "react-native-builder-bob": "^0.17.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-webview": "*"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ],
    "files": [
      "src/"
    ]
  }
}
